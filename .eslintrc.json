{
    "extends": [
        "eslint:recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 2022,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "rules": {
      "quotes": ["error", "double"]
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    }
}
